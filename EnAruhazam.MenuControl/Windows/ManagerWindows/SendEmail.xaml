<Window x:Class="EnAruhazam.MenuControl.Windows.ManagerWindows.SendEmail"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EnAruhazam.MenuControl.Windows.ManagerWindows"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <Label>Név</Label>
        <TextBox Name="NameInput"></TextBox>
        <Label>Email</Label>
        <TextBox Name="EmailInput"></TextBox>
        <Label>Tárgy</Label>
        <TextBox Name="SubjectInput"></TextBox>
        <TextBox Height="250" Name="DescInput"></TextBox>
        <Button Name="Submit" Click="Submit_Click">Küldés</Button>
    </StackPanel>
</Window>
