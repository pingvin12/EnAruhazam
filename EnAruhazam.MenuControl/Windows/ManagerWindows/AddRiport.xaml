<Window x:Class="EnAruhazam.MenuControl.AddRiport"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EnAruhazam.MenuControl.Windows.ManagerWindows"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <Label Content="Riport Date:" Margin="0,0,305,0"></Label>
        <DatePicker Margin="0,0,305,0" Name="RiportDate" />
        <Label Content="Equipment Name:" Margin="0,0,305,0"></Label>
        <ComboBox x:Name="SelectableEquipments" Height="25" Margin="0,0,305,0" />
        <Label Content="Description" Margin="0,0,305,0"></Label>
        <TextBox Name="Description" Height="100" Margin="0,0,305,0"></TextBox>
        <Button Width="100" Name="AddRip" Click="AddRip_Click" Margin="0,0,305,0" >Elküldés</Button>

    </StackPanel>
</Window>
