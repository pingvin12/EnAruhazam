<Window x:Class="EnAruhazam.AddPerson"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EnAruhazam"
        mc:Ignorable="d"
        Title="AddPerson" Height="318" Width="266">
    <StackPanel>

        <Label Content="Id:"></Label>
        <TextBox Name="Id"></TextBox>
        <Label Content="Person Name:"></Label>
        <TextBox Name="PersonName"></TextBox>
        <Label Content="JoinedDate:"></Label>
        <TextBox Name="JoinedDate"></TextBox>
        <Label Content="Password"></Label>
        <TextBox Name="Password"></TextBox>
        <Label Content="Phone:"></Label>
        <TextBox Name="Phone"></TextBox>
        <Label Content="Email:"></Label>
        <TextBox Name="Email"></TextBox>
        <Button Name="AddPer" Click="AddPer_Click" >Elküldés</Button>
    </StackPanel>
</Window>
