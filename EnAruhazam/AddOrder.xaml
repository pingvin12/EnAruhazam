<Window x:Class="EnAruhazam.AddOrder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EnAruhazam"
        mc:Ignorable="d"
        Title="AddOrder" Height="328" Width="216">
    <StackPanel>
        <Label Content="Id:"></Label>
        <TextBox Name="Id"></TextBox>
        <Label Content="Product Name:"></Label>
        <TextBox Name="ProductName"></TextBox>
        <Label Content="Expiration Date:"></Label>
        <TextBox Name="ExpirationDate"></TextBox>
        <Label Content="Row:"></Label>
        <TextBox Name="Row"></TextBox>
        <Label Content="Spot:"></Label>
        <TextBox Name="Spot"></TextBox>
        <Label Content="Price:"></Label>
        <TextBox Name="Price"></TextBox>
        <Button Name="AddOrd" Click="AddOrd_Click">Elküldés</Button>
        
    </StackPanel>
</Window>
